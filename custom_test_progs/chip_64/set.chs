# Tests whether set is working correctly and draws a start if yes

resolution 64 32
set 8 :0x19 0x56
call :halt
set 8 :0x35 :set_to
call :halt
halt: halt

set_to: create 8 0x75

x1: create 4 23
y1: create 4 12

x2: create 4 33
y2: create 4 12

draw:
	draw 9 9 :x1 :y1 :star
	present
ret

